<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xyz.leesfilm.photoMapper">
<select id="getTime" resultType="string">
select now()
</select>

  <insert id="insertPhoto" parameterType="com.xyz.leesfilm.VO.PhotoVO">
    insert into photo(photoId,photoAddr,photoCategory)
    values(#{photoId},#{photoAddr},#{photoCategory})
  </insert>
</mapper>